includes:
    - vendor/phpstan/phpstan-symfony/extension.neon
    - phpstan/symfony-configuration.php
    - phpstan-baseline.neon
    
parameters:
    level: max
    paths:
        - bin/
        - config/
        - public/
        - src/
        - tests/

    symfony:
        console_application_loader: phpstan/console-application.php
